<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Register</title>
</head>
<body>
    <h1>Register Form</h1>
    <form action="/users/edit" method="POST">
        <%data.forEach(el=>{%>
        <label for="id">ID</label></br>
        <input type="text" name="id" readonly value="<%=el.id%>"></br></br>
        <label for="firstName">First Name</label></br>
        <input type="text" name="firstName" value="<%=el.firstName%>"></br></br>
        <label for="lastName">Last Name</label></br>
        <input type="text" name="lastName" value="<%=el.lastName%>"></br></br>
        <label for="username">Username</label></br>
        <input type="text" name="username" value="<%=el.username%>"></br></br>
        <label for="email">Email</label></br>
        <input type="email" name="email" value="<%=el.email%>"></br></br>
        <label for="password">Password</label></br>
        <input type="password" name="password" value="<%=el.password%>"></br></br>
        <label for="phone">Phone Number</label></br>
        <input type="text" name="phone" value="<%=el.phone%>"></br></br>
        <label for="address">Address</label></br>
        <textarea name="address" rows="5" cols="20" value="<%=el.address%>"></textarea></br></br>
        <button>Edit</button>
        <%})%>
    </form>
</body>
</html>